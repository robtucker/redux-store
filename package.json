{
    "name": "@robtucker/redux-store",
    "version": "0.1.0",
    "description": "Configurable Redux store with rxjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/robtucker/redux-store"
    },
    "main": "./dist/index.js",
    "scripts": {
        "lint": "tslint -p ./tsconfig.json -c ./tslint.json -e '**/*.spec.ts' -e 'node_modules/**/*' -t json",
        "lint:version": "tslint -v",
        "lint:fix": "tslint -p ./tsconfig.json -c ./tslint.json -e '**/*.spec.ts' -e 'node_modules/**/*' --fix",
        "prepublishOnly": "npm test && rm -rf ./dist && npm run tsc:compile",
        "tsc:compile": "tsc",
        "tsc:watch": "tsc --watch --pretty",
        "tsfmt:verify": "tsfmt --verify",
        "tsfmt:replace": "tsfmt -r",
        "test": "jest"
    },
    "keywords": [],
    "author": "Rob Tucker <rob@coderlab.co.uk>",
    "license": "Unlicensed",
    "dependencies": {
        "@types/lodash": "^4.14.74",
        "@types/node": "^7.0.13",
        "@types/react": "15.0.38",
        "@types/react-router-redux": "5.0.3",
        "@types/redux-localstorage": "^1.0.2",
        "@types/redux-localstorage-debounce": "^0.1.1",
        "@types/redux-localstorage-filter": "^0.1.1",
        "@types/redux-logger": "^3.0.0",
        "lodash": "^4.17.4",
        "react": "16.0.0-alpha.12",
        "react-router-redux": "^5.0.0-alpha.6",
        "redux": "3.7.2",
        "redux-devtools-extension": "^2.13.2",
        "redux-localstorage": "^1.0.0-rc5",
        "redux-localstorage-debounce": "^0.1.0",
        "redux-localstorage-filter": "^0.1.1",
        "redux-logger": "^3.0.6",
        "redux-observable": "0.17.0",
        "redux-thunk": "^2.2.0",
        "rxjs": "5.5.2"
    },
    "devDependencies": {
        "@types/faker": "^4.1.0",
        "@types/jest": "19.2.2",
        "faker": "^4.1.0",
        "jest": "19.0.2",
        "ts-jest": "19.0.10",
        "tslib": "^1.7.1",
        "tslint": "^5.6.0",
        "tslint-react": "^3.2.0",
        "typescript": "2.6.2",
        "typescript-formatter": "^5.2.0"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ]
    }
}
